<Window x:Class="EGISSOEditor_2._0.Views.Windows.DialogProcedureWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EGISSOEditor_2._0.Views.Windows"
        mc:Ignorable="d"
        ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner"
        Title="DialogProcedureWindow" Width="400" SizeToContent="Height" MinHeight="100">
    <Grid>
        <Grid Margin="10,5">
            <StackPanel>
                <StackPanel Orientation="Horizontal" Margin="0,3">
                    <TextBlock Text="Текущий элемент:" Padding="0,0,5,0"/>
                    <TextBlock x:Name="tbCurrentProcessedElement"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal" Margin="0,3">
                    <TextBlock Text="Оставшиеся элементы:" Padding="0,0,5,0"/>
                    <TextBlock/>
                </StackPanel>
            </StackPanel>
            
        </Grid>
    </Grid>
</Window>
